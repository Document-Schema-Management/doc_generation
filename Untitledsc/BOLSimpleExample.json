{
    "schema": {
        "model": "BOLSimpleExample",
        "type": "object",
        "properties": {
            "BOLSimpleExample": {
                "type": "object",
                "properties": {
                    "Attribute1": {
                        "description": "missing description",
                        "type": "integer",
                        "format": "int32",
                        "default": "11223344"
                    },
                    "Attribute2": {
                        "description": "missing description",
                        "type": "integer",
                        "format": "int64"
                    },
                    "Attribute3": {
                        "description": "missing description",
                        "type": "number",
                        "format": "float"
                    },
                    "Attribute4": {
                        "description": "missing description",
                        "type": "number",
                        "format": "double"
                    },
                    "Attribute5": {
                        "description": "missing description",
                        "type": "string",
                        "format": "byte"
                    },
                    "Attribute6": {
                        "description": "missing description",
                        "type": "string",
                        "format": "binary"
                    },
                    "Attribute7": {
                        "description": "missing description",
                        "type": "string",
                        "format": "password"
                    },
                    "Attribute8": {
                        "description": "missing description",
                        "type": "string",
                        "default": "Example value"
                    }
                },
                "allOf": [
                    {
                        "$ref": "#/properties/TransportDocument"
                    },
                    {
                        "type": "object"
                    },
                    {
                        "$ref": "#/properties/Consignment"
                    },
                    {
                        "type": "object"
                    }
                ]
            },
            "Consignee": {
                "type": "object",
                "properties": {},
                "allOf": [
                    {
                        "$ref": "#/properties/Party"
                    },
                    {
                        "type": "object"
                    }
                ]
            },
            "Consignment": {
                "type": "object",
                "properties": {
                    "departureDate": {
                        "description": "Date and time of departure. ",
                        "type": "string",
                        "format": "date-time"
                    },
                    "serviceMode": {
                        "description": "The scope of the consignment, such as CY-CY or SD-SD.",
                        "type": "string"
                    },
                    "transportEquipmentQuantity": {
                        "description": "A number of pieces of transport equipment, such as containers or similar unit load devices, in this consignment.",
                        "type": "number"
                    },
                    "consignee": {
                        "$ref": "#/properties/Consignee"
                    },
                    "consignmentItems": {
                        "items": {
                            "$ref": "#/properties/ConsignmentItem"
                        },
                        "type": "array",
                        "minItems": 1
                    },
                    "consignmentReferences": {
                        "items": {
                            "$ref": "#/properties/Reference"
                        },
                        "type": "array"
                    },
                    "utilizedTransportEquipment": {
                        "items": {
                            "$ref": "#/properties/TransportEquipment"
                        },
                        "type": "array"
                    }
                },
                "required": [
                    "consignee",
                    "consignmentItems"
                ]
            },
            "ConsignmentItem": {
                "type": "object",
                "properties": {
                    "sequence": {
                        "description": "The sequence number for this consignment item.",
                        "type": "number"
                    },
                    "information": {
                        "description": "Information, expressed as text, for this consignment item.",
                        "type": "string"
                    },
                    "code": {
                        "description": "The harmonized system code specifying a type of consignment item.",
                        "type": "string"
                    }
                }
            },
            "Document": {
                "type": "object",
                "properties": {
                    "originatorName": {
                        "description": "The proper name (human readable) of the entity that published this event.",
                        "type": "string"
                    },
                    "originatorId": {
                        "description": "The party identification code (i.e. SCAC) of the organization publishing the event.",
                        "type": "string"
                    },
                    "issue": {
                        "description": "A date, time, date time or other date time value for an issuance of this document.",
                        "type": "string",
                        "format": "date-time"
                    },
                    "originalIssued": {
                        "description": "The number of originals issued for this document.",
                        "type": "number"
                    },
                    "copyIssued": {
                        "description": "The number of copies issued for this document.",
                        "type": "number"
                    },
                    "references": {
                        "items": {
                            "$ref": "#/properties/Reference"
                        },
                        "type": "array"
                    }
                }
            },
            "Party": {
                "type": "object",
                "properties": {
                    "name": {
                        "description": "A name, expressed as text, for this party.",
                        "type": "string"
                    },
                    "printedParty": {
                        "description": "The party name and address exactly as printed on the document.",
                        "type": "string"
                    },
                    "registeredForVAT": {
                        "description": "VAT registration indicator. ",
                        "type": "boolean"
                    },
                    "taxReference": {
                        "description": "Tax identification of the party.",
                        "type": "string"
                    },
                    "partyRef": {
                        "description": "Reference which represents the party.",
                        "type": "string"
                    },
                    "organizationId": {
                        "description": "Unique identifier of the party. ",
                        "type": "string"
                    }
                }
            },
            "Reference": {
                "type": "object",
                "properties": {
                    "type": {
                        "description": "Type of reference. ",
                        "type": "string",
                        "enum": [
                            "proformaInvoiceNumber",
                            "deliveryScheduleNumber",
                            "consigneeShipmentReferenceNumber",
                            "despatchNoteNumber",
                            "customsTariffNumber",
                            "customsDecisionRequestNumber",
                            "subHouseBillOfLadingNumber",
                            "customsGuaranteeNumber",
                            "sellersCatalogueNumber",
                            "declarantsCustomsIdentityNumber",
                            "importerReferenceNumber",
                            "customsDeclarationNumber",
                            "stockKeepingUnitNumber",
                            "adviceOfCollectionNumber",
                            "railwayWagonNumber",
                            "buyersItemNumber",
                            "customsHouseBrokerReferenceNumber",
                            "reportNumber",
                            "traderAccountNumber",
                            "governmentAgencyReferenceNumber",
                            "regristrationNumberOfPreviousCustomsDeclaration",
                            "productReservationNumber",
                            "customsItemNumber",
                            "tariffNumber",
                            "replenishmentPurchaceOrderNumber",
                            "contractPartyReferenceNumber",
                            "agentsReferenceNumber",
                            "deliveryVerificationCertificate",
                            "customsReleaseCode",
                            "complianceCodeNumber",
                            "ministerialCertificateOfHomologation",
                            "banksCommonTransactionReferenceNumber",
                            "customsTranshipmentNumber",
                            "vehicleIdentificationNumber",
                            "replenishmentPurchaseOrderRangeStartNumber",
                            "consigneesInvoiceNumber",
                            "replenishmentPurchaseOrderRangeEndNumber",
                            "buyersCatalogueNumber",
                            "inlandTransportOrderNumber",
                            "consigneesReference",
                            "usCustomsServiceEntryCode",
                            "transportEquipmentReleaseOrderReference",
                            "importersLetterOfCreditReference",
                            "customsBindingRulingNumber",
                            "customsNonBindingRulingNumber",
                            "uniqueGoodsShipmentIdentifier",
                            "rateCodeNumber",
                            "airWaybillNumber",
                            "buyersContractNumber",
                            "houseBillOfLadingNumber",
                            "billOfLadingNumber",
                            "bookingReferenceNumber",
                            "consigneesOrderNumber",
                            "carriersReferenceNumber",
                            "contractNumber",
                            "buyersDebtorNumber",
                            "exportDeclaration",
                            "exportPermitNumber",
                            "containerEquipmentReceiptNumber",
                            "exportersReferenceNumber",
                            "exportLicenceNumber",
                            "freightForwardersReferenceNumber",
                            "governmentReferenceNumber",
                            "harmonizedSystemNumber",
                            "houseWaybillNumber",
                            "inBondNumber",
                            "iataCargoAgentCodeNumber",
                            "importLicenceNumber",
                            "invoiceNumber",
                            "letterOfCreditNumber",
                            "shipNoticeManifestNumber",
                            "masterBillOfLadingNumber",
                            "manufacturersPartNumber",
                            "manufacturersOrderNumber",
                            "masterAirWaybillNumber",
                            "northAmericanHazardousGoodsClassificationNumber",
                            "purchaseOrderNumber",
                            "generalReferenceNumber",
                            "pickupReferenceNumber",
                            "plantNumber",
                            "packingListNumber",
                            "purchaseOrderResponseNumber",
                            "purchaseOrderChangeNumber",
                            "paymentReference",
                            "priceQuoteNumber",
                            "shippersIdentifyingNumberForShipment",
                            "sealNumber",
                            "shipmentReferenceNumber",
                            "sellersReferenceNumber",
                            "truckersBillOfLading",
                            "transactionReferenceNumber",
                            "orderNumber",
                            "warehouseEntryNumber",
                            "warehouseReceiptNumber",
                            "warehouseStorageLocationNumber",
                            "cargoControlNumber",
                            "previousCargoControlNumber",
                            "mutuallyDefinedReferenceNumber"
                        ]
                    },
                    "value": {
                        "description": "Reference value. ",
                        "type": "string"
                    }
                }
            },
            "TransportDocument": {
                "type": "object",
                "properties": {
                    "shippedOnBoardDate": {
                        "description": "Date when the cargo was shipped on board. ",
                        "type": "string",
                        "format": "date-time"
                    },
                    "billOfLadingNumber": {
                        "description": "Bill of lading number of the transport document. ",
                        "type": "string"
                    },
                    "bookingNumber": {
                        "items": {
                            "description": "Booking number(s). ",
                            "type": "string"
                        },
                        "type": "array"
                    }
                },
                "allOf": [
                    {
                        "$ref": "#/properties/Document"
                    },
                    {
                        "type": "object"
                    }
                ],
                "required": [
                    "billOfLadingNumber"
                ]
            },
            "TransportEquipment": {
                "type": "object",
                "properties": {
                    "equipmentNumber": {
                        "description": "Unique identifier for this piece of transport equipment, printed physically on the equipment.",
                        "type": "string"
                    },
                    "equipmentType": {
                        "description": "ISO 6346 size and type code (for example \"45G1\")",
                        "type": "string"
                    },
                    "printedEquipmentType": {
                        "description": "The container size and type, as printed on the transport document (for example \"40 DRY 9''6\").",
                        "type": "string"
                    },
                    "code": {
                        "items": {
                            "description": "The harmonized system code specifying a type of loaded consignment item(s).",
                            "type": "string"
                        },
                        "type": "array"
                    }
                },
                "required": [
                    "equipmentNumber"
                ]
            }
        }
    }
}